@{
  ViewData["Title"] = "Tours Page";
}
<link href="~/css/tours.css" rel="stylesheet" />
<div class="container-fluid m-0 p-0">
  <div class="hero page hero-carousel">
    <div id="carouselExampleInterval" class="carousel slide" data-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active" data-interval="1500">
          <img src="~/images/tours/1.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item" data-interval="1500">
          <img src="~/images/tours/2.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
          <img src="~/images/tours/3.jpg" class="d-block w-100" alt="...">
        </div>
      </div>
      <a class="carousel-control-prev icon" href="#carouselExampleInterval" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselExampleInterval" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>
</div>
<div class="container choose-tour">
  <div class="row">
    <div class="col-sm-12">
      <div class="heading tours">
        <h2>Обери тур на будь який смак</h2>
      </div>
    </div>
  </div>
</div>
<section class="container select-country">
  <div class="row">
    <div class="col-sm-6">
      <h5>Обери країну</h5>
      <div class="version-select previous-versions-cdgs">
        <select class="form-control form-control-lg" id="method-select">
          <option id="ptr-all-select">Всі</option>
          <option id="ptr-albania-select">Албанія</option>
          <option id="ptr-bulgaria-select">Болгарія</option>
          <option id="ptr-greece-select">Греція</option>
          <option id="ptr-georgia-select">Грузія</option>
          <option id="ptr-egypt-select">Египет</option>
          <option id="ptr-spain-select">Іспанія</option>
          <option id="ptr-italy-select">Італія</option>
          <option id="ptr-cyprus-select">Кіпр</option>
          <option id="ptr-maldivi-select">Мальдіви</option>
          <option id="ptr-oau-select">ОАЕ</option>
          <option id="ptr-tanzania-select">Танзанія</option>
          <option id="ptr-turkey-select">Турція</option>
          <option id="ptr-ukraine-select">Україна</option>
          <option id="ptr-france-select">Франція</option>
          <option id="ptr-croatia-select">Хорватія</option>
          <option id="ptr-chornogorya-select">Чорногорія</option>
          <option id="ptr-sri-lanka-select">Шрі-Ланка</option>
        </select>
      </div>
    </div>
  </div>
</section>
